class Funcionario:
    def __init__(self, nome, salario):
        self.nome = nome
        self.salario = salario

    def getNome(self):
        return self.nome

    def getSalario(self):
        return self.salario

if __name__ == '__main__':
    funcionario1 = Funcionario('João', 2000.0)
    funcionario2 = Funcionario('Maria', 3000.0)

    print(f"Nome: {funcionario1.getNome()}, Salário: {funcionario1.getSalario()}")
    print(f"Nome: {funcionario2.getNome()}, Salário: {funcionario2.getSalario()}")
